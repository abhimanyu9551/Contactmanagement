<%= form_for @contact do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :email %>
  <%= f.text_field :email %><br>

  <%= f.label :phone %>
  <%= f.text_field :phone %><br>

  <%= f.label :address %>
  <%= f.text_field :address %><br>

  <%= f.label :brief_notes %>
  <%= f.text_field :brief_notes %><br>

  <<button type="submit" name="button">Save</button>
<% end %>
